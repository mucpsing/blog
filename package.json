{
  "name": "blog-docusaurus-2",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "tsc -p ./scripts & docusaurus start",
    "start:scripts": "tsc -p ./scripts",
    "watch:scripts": "tsc -p ./scripts --watch",
    "build": "tsc -p ./scripts & docusaurus build & cps -c ./build",
    "compress": "cps -c ./build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "tstest": "tsc -p ./tsconfig.json"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@docusaurus/core": "^2.4.0",
    "@docusaurus/module-type-aliases": "^2.4.0",
    "@docusaurus/plugin-ideal-image": "^2.4.0",
    "@docusaurus/preset-classic": "^2.4.0",
    "@docusaurus/theme-mermaid": "^2.4.0",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.2.1",
    "lodash": "^4.17.21",
    "prism-react-renderer": "^1.3.5",
    "rc-banner-anim": "^2.4.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tailwindcss": "^3.2.7",
    "typed.js": "^2.0.12"
  },
  "devDependencies": {
    "@popperjs/core": "^2.11.7",
    "@tsconfig/docusaurus": "^1.0.5",
    "@types/glob": "^8.0.1",
    "@types/globby": "^9.1.0",
    "@types/lodash": "^4.14.191",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.13",
    "commitizen": "^4.3.0",
    "commitlint-config-cz": "^0.13.3",
    "cz-customizable": "^7.0.0",
    "glob": "^8.1.0",
    "globby": "^13.1.4",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^8.0.1",
    "rc-queue-anim": "^2.0.0",
    "react-popper": "^2.3.0",
    "stylus": "^0.59.0",
    "stylus-loader": "^7.1.0",
    "typescript": "^4.7.4",
    "webp-converter": "^2.3.3",
    "yaml": "^2.2.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
